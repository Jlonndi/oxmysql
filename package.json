{
  "name": "oxmysql",
  "version": "2.0.0",
  "description": "FXServer to MySQL communication via node-mysql2",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "webpack --mode production",
    "watch": "webpack --watch --mode development",
    "build:es": "esbuild --bundle --platform=node source/server/index.ts --outfile=dist/server/build.js",
    "watch:es": "esbuild --watch --bundle --platform=node source/server/index.ts --outfile=dist/server/build.js",
    "postinstall": "patch-package"
  },
